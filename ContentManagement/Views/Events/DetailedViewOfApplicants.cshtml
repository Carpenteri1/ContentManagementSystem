@{
}
<div>
</div><div class="container">
    <div class="row">
        <div class="col-7">
            <div style="height:1.5vw;"></div>
            <h4>Detaljerad Information</h4>
            <hr />
            <div style="font-size:0.6em;">
                <p style="margin-bottom:0.5em;"><span style="font-weight:bold;">Namn:</span> @Model.FirstName @Model.SurName</p>
                <p style="margin-bottom:0.5em;"><span style="font-weight:bold;">Golf-ID:</span> @Model.GolfId</p>
                <p style="margin-bottom:0.5em;"><span style="font-weight:bold;">Hemmaklubb:</span> @Model.HomeClubName</p>
                <p style="margin-bottom:0.5em;"><span style="font-weight:bold;">Telefon:</span> @Model.Phone</p>
                <p><span style="font-weight:bold;">E-post:</span> @Model.Email</p>
                @if (Model.GoodToknowInfo != null)
                {
                    <p style="font-weight:bold;margin-bottom:0.5em;">Bra att veta:</p>
                    <p style="border:1px solid black;">@Model.GoodToknowInfo</p>
                }
            </div>
             <hr />
                <div class="form-group">
                    <div class="row">
                        <div class="col-6">
                            @Html.HiddenFor(item => item.applyedToEvent.Id)
                            <button type="button" class="btn text-white p-0 text-center" onclick="location.href='@Url.Action("Applicants", "Events", new { id = TempData["EventId"].ToString()})'" style="font-size: 0.4vw;width:3.8vw;height:1.2vw;background-color:#0293fc;">Tillbaka</button>
                        </div>
                        <div class="col-6 text-right">
                         
                        </div>
                    </div>
                </div>
        </div>
        <div class="col-auto"></div>
    </div>
</div>
